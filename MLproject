entry_points:
    main:
        parameters:
            k: {type: int, default: 10}
            train_path: path
            test_path: path
            seed: {type: int, default: 1}
            weights: {type: string, default: "0.3,0.3,0.3"}
            normalization: string
            algorithm: string
        command: "python3 main.py --k {k} --train_path {train_path} --test_path {test_path} --seed {seed} --weights {weights} --normalization {normalization} --algorithm {algorithm}"
